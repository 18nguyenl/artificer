  "type": "module",
  "main": "./dist/artificer.min.js",
  "exports": {
    "import": "./dist/artificer.module.js",
    "require": "./dist/artificer.min.js"
  },
  "scripts": {
    "build:module": "esbuild index.ts --bundle --format=esm --outfile=dist/artificer.module.js --target=es2020",
